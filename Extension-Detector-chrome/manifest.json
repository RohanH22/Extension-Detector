

{
    "manifest_version": 2,
    "name": "Extension-Detector-chrome",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://im.piruby.com/", "https://industrymitra.piruby.com/", "https://ipreneur.piruby.com/", "https://innosential.industry-mitra.com/"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["management", "tabs", "runtime"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "search.png"
    }, 
    "icons": {
        "128": "search.png"
    }
  }